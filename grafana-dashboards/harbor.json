{
  "dashboard": {
    "title": "HPC Cluster - Harbor Dashboard",
    "tags": ["hpc", "harbor", "registry"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Images Total",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "harbor_images_total",
            "refId": "A"
          }
        ]
      },
      {
        "id": 2,
        "title": "Pulls per Second",
        "type": "graph",
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(harbor_pulls_total[5m])",
            "refId": "A",
            "legendFormat": "Pulls/sec"
          }
        ]
      }
    ]
  }
}
