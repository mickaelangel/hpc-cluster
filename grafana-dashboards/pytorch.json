{
  "dashboard": {
    "title": "HPC Cluster - PyTorch",
    "tags": ["hpc", "pytorch", "ml"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Training Jobs",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "pytorch_training_jobs_running",
            "refId": "A"
          }
        ]
      },
      {
        "id": 2,
        "title": "Training Accuracy",
        "type": "graph",
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "pytorch_training_accuracy",
            "refId": "A",
            "legendFormat": "Accuracy"
          }
        ]
      }
    ]
  }
}
