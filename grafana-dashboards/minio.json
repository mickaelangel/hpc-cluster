{
  "dashboard": {
    "title": "HPC Cluster - MinIO Dashboard",
    "tags": ["hpc", "minio", "storage"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Buckets Total",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "minio_buckets_total",
            "refId": "A"
          }
        ]
      },
      {
        "id": 2,
        "title": "Objects Total",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "minio_objects_total",
            "refId": "A"
          }
        ]
      },
      {
        "id": 3,
        "title": "Storage Used",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "minio_storage_used_bytes",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes"
          }
        }
      },
      {
        "id": 4,
        "title": "I/O Operations",
        "type": "graph",
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(minio_io_operations_total[5m])",
            "refId": "A",
            "legendFormat": "Ops/sec"
          }
        ]
      }
    ]
  }
}
