{
  "dashboard": {
    "title": "HPC Cluster - Artifactory Dashboard",
    "tags": ["hpc", "artifactory", "artifacts"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Artifacts Total",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "artifactory_artifacts_total",
            "refId": "A"
          }
        ]
      },
      {
        "id": 2,
        "title": "Downloads per Second",
        "type": "graph",
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(artifactory_downloads_total[5m])",
            "refId": "A",
            "legendFormat": "Downloads/sec"
          }
        ]
      }
    ]
  }
}
