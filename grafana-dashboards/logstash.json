{
  "dashboard": {
    "title": "HPC Cluster - Logstash Dashboard",
    "tags": ["hpc", "logstash", "logging"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Events Processed",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "logstash_events_total",
            "refId": "A"
          }
        ]
      },
      {
        "id": 2,
        "title": "Events per Second",
        "type": "graph",
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(logstash_events_total[5m])",
            "refId": "A",
            "legendFormat": "Events/sec"
          }
        ]
      }
    ]
  }
}
