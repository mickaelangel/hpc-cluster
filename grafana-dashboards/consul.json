        "targets": [
          {
            "expr": "consul_services_registered",
            "refId": "A"
          }
        ]
      },
      {
        "id": 2,
        "title": "Nodes Total",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "consul_nodes_total",
            "refId": "A"
          }
        ]
      }
    ]
  }
}
