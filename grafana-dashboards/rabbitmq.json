{
  "dashboard": {
    "title": "HPC Cluster - RabbitMQ Dashboard",
    "tags": ["hpc", "rabbitmq", "messaging"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Queues Total",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rabbitmq_queues_total",
            "refId": "A"
          }
        ]
      },
      {
        "id": 2,
        "title": "Connections",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "rabbitmq_connections_total",
            "refId": "A"
          }
        ]
      },
      {
        "id": 3,
        "title": "Messages Published",
        "type": "graph",
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(rabbitmq_messages_published_total[5m])",
            "refId": "A",
            "legendFormat": "Published/sec"
          }
        ]
      }
    ]
  }
}
